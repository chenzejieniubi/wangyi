AA,QAAA,KAAA,IAAA,CAAA,YAAA,KAAA,QAAA,EAAA;AACA,eAAA,KAAA,IAAA,CAAA,YAAA;AACA,OAFA,MAEA;AACA,eAAA,EAAA;AACA;AACA;AAnCA,GATA;AA8CA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA,GA9CA;AAiDA,EAAA,OAjDA,qBAiDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAEA,KAAA,CAAA,GAAA,CAAA,GAAA,CAAA,QAAA,CAFA;;AAAA;AAEA,cAAA,GAFA;AAGA;AACA,cAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AAtDA,CAAA","sourcesContent":["<template>\n<!--  -->\n  <div id=\"home\" class=\"home\">\n    <van-search v-model=\"searchData\" placeholder=\"商品搜索 共239款好物\"  input-align=\"center\"/>\n    <van-swipe :autoplay=\"3000\" :width=\"375\" :height=\"200\"> \n       <!-- 为什么要动态？？ -->\n      <van-swipe-item v-for=\"(image, index) in images\" :key=\"index\">\n        <img class=\"swiperimg\" v-lazy=\"image.image_url\" />\n      </van-swipe-item>\n    </van-swipe>\n\n    <!-- 5个图标 -->\n    <van-grid :column-num=\"5\">\n      <van-grid-item v-for=\"(item,index) in channel\" :key=\"index\" :icon=\"item.icon_url\" :text=\"item.name\" />\n    </van-grid>\n\n\n    <div class=\"cellspan\">\n    <!-- <van-cell center   value=\"品牌制造商直供\"  size=\"large\"  /> -->\n      <div class=\"top\">品牌制造商直供</div>\n      <div class=\"brandlist\">\n      <van-grid :column-num=\"2\">\n        <van-grid-item v-for=\"(item1,index1) in brandList\" :key=\"index1\" icon=\"photo-o\" text=\"文字\">\n          <van-image lazy-load :src=\"item1.new_pic_url\" />\n          <h4 class=\"title\">{{item1.name}}</h4>\n          <p class=\"price\">{{item1.floor_price}}元起</p>\n        </van-grid-item>\n      </van-grid>\n      </div>\n    </div>\n\n\n\n\n\n    <div class=\"cellspan\">\n    <!-- <van-cell center   value=\"新品首发\"  size=\"large\"  /> -->\n      <div class=\"top\">新品首发</div>\n      <div class=\"newlist\">\n      <van-grid :column-num=\"2\">\n        <van-grid-item v-for=\"(item2,index2) in newGoodsList\" :key=\"index2\" icon=\"photo-o\" text=\"文字\">\n          <van-image lazy-load :src=\"item2.list_pic_url\" />\n          <h4 class=\"title\">{{item2.name}}</h4>\n          <p class=\"price\">{{item2.retail_price}}元起</p>\n        </van-grid-item>\n      </van-grid>\n      </div>\n    </div>\n\n\n\n    \n\n    <div class=\"cellspan\">\n    <!-- <van-cell center   value=\"人气面板\"  size=\"large\"  /> -->\n      <div class=\"top\">人气面板</div>\n      <div class=\"hotlist\">\n      <van-card v-for=\"(item3,index3) in hotGoodsList\" :key=\"index3\"\n        :price=\"item3.retail_price\"\n        :desc=\"item3.goods_brief\"\n        :title=\"item3.name\"\n        :thumb=\"item3.list_pic_url\"\n      />\n      </div>\n    </div>\n    <div style=\"height:50px;\"></div>\n    \n    <tab-btn> </tab-btn>\n\n\n\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport axios from 'axios'\nimport api from '../assets/config/api'\nimport Vue from 'vue';\nimport { Lazyload } from 'vant';\nimport tabBtn from '@/components/tabBtn.vue';\n\nVue.use(Lazyload);\nexport default {\n  name: 'home',\n  data:function(){\n    return {\n      searchData:'',\n      data:{},\n      tabActive:0\n    }\n  },\n  computed: {\n    images:function(){\n      if(typeof this.data.banner == 'object'){\n        return this.data.banner\n      }else{\n        return []\n      }\n    },\n    channel:function(){\n      if(typeof this.data.channel == 'object'){\n        return this.data.channel\n      }else{\n        return []\n      }\n    },\n    brandList:function(){\n      if(typeof this.data.brandList == 'object'){\n        return this.data.brandList\n      }else{\n        return []\n      }\n    },\n    newGoodsList:function(){\n      if(typeof this.data.newGoodsList == 'object'){\n        return this.data.newGoodsList\n      }else{\n        return []\n      }\n    },\n    hotGoodsList:function(){\n      if(typeof this.data.hotGoodsList == 'object'){\n        return this.data.hotGoodsList\n      }else{\n        return []\n      }\n    },\n  },\n  components: {\n    tabBtn\n  },\n async mounted() {\n    //console.log(api)\n    let res = await axios.get(api.IndexUrl)\n    //console.log(res.data)\n    this.data = res.data.data;\n  },\n}\n</script>\n\n<style lang=\"less\">\n  #home{\n    .van-grid-item{\n      overflow: hidden;\n    }\n    .swiperimg{\n      width: 375px;\n      height: 200px;\n    }\n    .cellspan .top{\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .brandlist{\n      .van-grid-item__content{\n        padding: 0;\n      }\n      .van-image{\n        border: 1px solid #ffffff;\n      }\n      .title{\n        position: absolute;\n        top: 20px;\n        left: 10px;\n      }\n      .price{\n        position: absolute;\n        top: 40px;\n        left: 10px;\n        font-size: 14px;\n        color: #999;\n      }\n    }\n    .newlist{\n      .title{\n        width: 90%;\n        overflow: hidden;\n        white-space: nowrap;\n      }\n      .price{\n        font-size: 14px;\n        color: #999;\n      }\n    }\n  }\n</style>\n"],"sourceRoot":"src/views"}]}