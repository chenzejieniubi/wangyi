A,KAbA,CAcA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA;;AA3BA,GAnBA;AAgDA,EAAA,OAAA,EAAA;AACA,IAAA,WAAA,EAAA,qBAAA,KAAA,EAAA;AACA;AACA,WAAA,WAAA,GAAA,KAAA;AACA;AAJA,GAhDA;AAsDA,EAAA,KAAA,EAAA;AACA,IAAA,WAAA;AAAA,iFAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KAAA,KAAA,CAAA,MAAA,KAAA,CADA;AAAA;AAAA;AAAA;;AAEA;AACA;AACA,gBAAA,EAJA,GAIA,KAAA,KAAA,CAAA,KAAA,WAAA,EAAA,EAJA;AAKA,gBAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AALA;AAAA,uBAMA,KAAA,CAAA,GAAA,CAAA,GAAA,CAAA,cAAA,EAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAAA,iBAAA,CANA;;AAAA;AAMA,gBAAA,GANA;AAOA;AACA,qBAAA,eAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,eAAA,CAAA,eAAA;AACA,qBAAA,SAAA,GAAA,KAAA,KAAA,CAAA,KAAA,WAAA,EAAA,OAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,eAAA;AAVA;AAAA;;AAAA;AAYA,qBAAA,eAAA,GAAA,EAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AADA,GAtDA;AAuEA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AAvEA,CAAA","sourcesContent":["<template>\r\n    <div id=\"category\">\r\n        <van-search v-model=\"searchData\" placeholder=\"商品搜索 共239款好物\"  input-align=\"center\"/>\r\n\r\n        <van-tree-select\r\n        :items=\"items\"\r\n        :main-active-index.sync=\"activeIndex\"\r\n        height='calc(100vh - 104px)'\r\n        @click-nav=\"changeRight\"\r\n        >\r\n        <template slot=\"content\">\r\n            <div class=\"imgbanner\">\r\n            <img width=\"100%\" :src=\"bannerImg\" alt=\"\">\r\n            </div>\r\n            <van-grid :column-num=\"3\">\r\n                <van-grid-item v-for =\"(item,index) in subCategoryList\" :icon=\"item.wap_banner_url\" :text=\"item.name\" :key=\"index\" \r\n                :to= \" '/categorylist/' + item.id \" />\r\n            </van-grid>\r\n        </template>\r\n\r\n        </van-tree-select>\r\n        <tab-btn></tab-btn>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport tabBtn from '@/components/tabBtn.vue';\r\nimport axios from 'axios'\r\nimport api from '@/assets/config/api.js'\r\n\r\nexport default {\r\n    data(){\r\n        return{\r\n            searchData:\"\",\r\n            // items: [{ text: '分组 1' }, { text: '分组 2' }],\r\n            activeIndex: 0,\r\n            subCategoryList:[],\r\n            data:{},\r\n            bannerImg:'',\r\n        }\r\n    },\r\n    async created(){\r\n        let res = await axios.get(api.CatalogList)\r\n        let data = res.data;\r\n        this.data =data.data;\r\n        this.subCategoryList = this.data.currentCategory.subCategoryList;\r\n        this.bannerImg = this.data.currentCategory.img_url;\r\n        console.log(data)\r\n    },\r\n    computed:{\r\n        items:function() {\r\n            // items: [{ text: '分组 1' }, { text: '分组 2' }],\r\n            if(this.data.categoryList==undefined){\r\n                return []\r\n            }else{\r\n                let arr =[]\r\n                this.data.categoryList.forEach((item,index) => {\r\n                    item.text = item.name;\r\n                    arr.push(item)\r\n                }) \r\n                return arr;\r\n            }\r\n        },\r\n        // subCategoryList:async function(){\r\n        //     if(this.items.length!==0){\r\n        //         // console.log(this.activeIndex)\r\n        //         // console.log(this.items)\r\n        //         let id =this.items[this.activeIndex].id\r\n        //         console.log(id)\r\n        //         let res = await axios.get(api.CatalogCurrent,{id})\r\n        //         console.log(res.data)\r\n        //         return []\r\n        //     }else{\r\n        //         return []\r\n        //     }\r\n            \r\n        // }\r\n    },\r\n    methods:{\r\n        changeRight:function(index){\r\n           // console.log(index)\r\n            this.activeIndex = index;\r\n        }\r\n    },\r\n    watch:{\r\n        activeIndex:async function(){\r\n            if(this.items.length!==0){\r\n                // console.log(this.activeIndex)\r\n                // console.log(this.items)\r\n                let id =this.items[this.activeIndex].id\r\n                console.log(id)\r\n                let res = await axios.get(api.CatalogCurrent,{params:{id}})\r\n                // console.log(res.data)\r\n                this.subCategoryList = res.data.data.currentCategory.subCategoryList\r\n                this.bannerImg = this.items[this.activeIndex].img_url;\r\n                console.log(this.subCategoryList)\r\n            }else{\r\n                this.subCategoryList = []\r\n            }\r\n        }\r\n    },\r\n    components:{\r\n        tabBtn\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"less\">\r\n    #category{\r\n        .imgbanner{\r\n            width: 100%;\r\n            box-sizing: border-box;\r\n            padding: 10px;\r\n        }\r\n    }\r\n</style>"],"sourceRoot":"src/views"}]}